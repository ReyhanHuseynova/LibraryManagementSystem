@model Rack
<div class="col-sm-12 col-xl-9" style="margin-top:30px; margin-left:50px">
    <div class="bg-light rounded h-100 p-4">
        <h6 class="mb-4">Create Rack</h6>
        <form method="post">
            <div class="mb-3">
                <label asp-for="RackNumber" class="form-label"></label>
                <input asp-for="RackNumber" class="form-control">
                <span asp-validation-for="RackNumber" class="text-danger"></span>
               
            </div>
            
            
            <div>
                <button type="submit" class="btn btn-primary">Create</button>
                <a asp-action="Index" class="btn btn-dark">Back</a>
            </div>

        </form>
    </div>
</div>

